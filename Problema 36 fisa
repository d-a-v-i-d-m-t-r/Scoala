#include <iostream>
#include <cstring>
#include <stdlib.h>
#include <fstream>

using namespace std;

int n, p;
int x[99];
int w = 0;
int modul(int a,int b)
{
if(a-b==1 || b-a==1)
    return 1;
else
    return 0;
}
int valid(int k) {
    if(k>=2 && x[k-1]==1 && x[k]==1)
        return 0;
    return 1;
}

int sol(int k) {
    return k == n;
}

void afis(int k) {
    for (int i = 1; i <= k; i++)
        cout << x[i] << " ";
    cout << "\n";
    w++;
}

void back(int k) { //se apeleaza back(1);
    x[k] = -1;
    while (x[k] <1 && k<=n) {
        x[k]++;
        if (valid(k))
            if (sol(k))
                afis(k);
            else
                back(k + 1);
    }
}


int main()
{
    cout << "n=";
    cin >> n;
    back(1);
    cout << w;
    return 0;
}
