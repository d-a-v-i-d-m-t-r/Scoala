#include <iostream>

using namespace std;

int  p,x,n;
//n e numarul de cifre din multime
//x e numarul dat
int s[99];
int w = 0;
int v[99];

int nr_format(int k)
{ int nr=0;
    for(int i=1;i<=k;i++)
        nr=nr*10+v[s[i]];
    return nr;
}

int nrc(int s)
{
    if(s==0)
      return 0;
    else
      return 1 + nrc(s/10);
}
int valid(int k)
{
    if(k==1 && v[s[k]]==0)
      return 0;
    if(nr_format(k)>=n)
        return 0;
    return 1;
}

int sol(int k)
{
  return nr_format(k) < x && k==p;
}

void afis(int k)
{
for (int i = 1; i <= k; i++)
    cout << v[s[i]];
    cout << "\n";
    w++;
}

void back(int k) {
s[k] = 0;
    while (s[k] < n && k<=nrc(x))
    {
            s[k]++;
            if (valid(k))
                if (sol(k))
                    afis(k);
                else
                  back(k+1);
    }
}

int main()
{
cout << " Numarul de cifre : "; cin>>n;
cout << " Cifrele sunt : "<< endl;
for(int i=1;i<=n;i++)
    cin>>v[i];

cout<<"Numarul este:"; cin>>x;
for(int i=1;i<=nrc(x);i++)
{
  p=i;
  back(1);
}
  cout<<"Numarul de numere generate="<<w;
return 0;
}
